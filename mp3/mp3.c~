#include <stdio.h>
#include <stdlib.h>

int main(void)
{
  int row;

  printf("Enter a row index: ");
  scanf("%d",&row);

  // Write your code here
  long int x = row;
  long int counter = row + 1;
 

  long int num = 1;
  long int adder = 1;
  while(counter>0){
      printf("%ld",num);
      printf(" ");
      num = num*x;
      num = num/adder;
      adder++;
      counter--;
      x--;
  }
 
 
  return 0;
}
